<div class="row margin-box">
  <div class="span7">
    <div class="rent_process">
      <div style='margin:0 30px;width:560px;'>
        <%= image_tag 'info_step_4.png' %>
      </div>
      <ul class="rent_steps">
        <li>选择车辆</li>
        <li>选择日期</li>
        <li>出租费用</li>
        <li class="step_active">出租完成</li>
      </ul>
    </div>
  </div>
  <div class="span4">
    <%= link_to '返回我的出租', myinfo_path, class: 'btn btn-success btn-large' %>
  </div>
  <div class="row-fluid info-detail">
  <div class="span7">
    <div class="withborder"> 
      <%= render partial: 'cars/pic_desc', object: @info.car, as: :car %>
      开始日期:<%= @info.rent_start %><br>
      结束日期:<%= @info.rent_end %><br>
    </div>
  </div>
  <div class="span4">

    <div id="request-box">
      <%= render partial: 'infos/rent_rates', object: @info, as: :info %>
    </div>

  </div>
</div>
</div>
