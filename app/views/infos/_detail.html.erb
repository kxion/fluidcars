<div class='row'>
  <div class='span6'>
    <table class='table'>
      <tr><%= image_tag(@info.avatar_url) %></tr>
      <tr><td>City:</td><td><%= @info.city %></td></tr>
      <tr><td>Location:</td><td><%= @info.location %></td></tr>
      <tr><td>Start:</td><td><%= @info.rent_start %></td></tr>
      <tr><td>End:</td><td><%= @info.rent_end %></td></tr>
      <tr><td>Price:</td><td><%= @info.price %></td></tr>
      <tr><td>Owner:</td><td><%= @info.user.name %></td></tr>
    </table>
  </div>
  <div class='span6'>
    <%= render :partial => 'shared/location', object: @info %>
  </div>
</div>