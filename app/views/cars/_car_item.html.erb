<tr>
  <td>
    <ul class="thumbnails">
      <li class="span2">
        <a href="<%= car_path(car) %>" class="thumbnail"> 
          <%= image_tag(car.avatar.thumb.url) %>
        </a>
      </li>
    </ul>
  </td>
  <td>
    <ul class="span2">
      <li><%= car.band %></li>
      <li><%= car.city %></li>
      <li><%= car.location %></li>
    </ul>
  </td>
  <td>
    <div class="span2">
      <div>
        <%= link_to '编辑', edit_car_path(car), class: 'btn btn-large btn-info' %>
      </div>
      <div>
        <%= link_to '出租', new_info_path(car_id: car.id), class: 'btn btn-large btn-success' %>
      </div>
    </div>
  </td>
</tr>
