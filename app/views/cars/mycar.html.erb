<div class="page-header">
  <h1>我的车辆</h1>
</div>
<div class="row car-list">
  <div class="span7">
    <% unless @cars.empty? %>
    <table class="table table-hover">
      <%= render partial: 'car_list_item', collection: @cars, as: :car %>
    </table>
    <%= will_paginate @cars %>
    <% else %>
    暂无车辆信息
    <% end %>
  </div>
  <div class="span4">
    <%= link_to '创建新车辆信息', new_car_path, class:'btn btn-large btn-success' %>
  </div>
</div>


