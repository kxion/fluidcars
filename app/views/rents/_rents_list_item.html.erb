<tr>
  <td>
    <ul class="thumbnails">
      <li class="span2">
        <a href="<%= show_rent_path(id: rent.id) %>">
          <%= image_tag(rent.car.picture.thumb.url, class: 'thumbnail') %>
        </a>
      </li>
    </ul>
  </td>
  <td>
    <ul class="span2 abstract_rent">
      <li><%= rent.car.brand %></li>
      <li><%= rent.car.location.city %><%= rent.car.location.detail %></li>
      <li>开始时间：<%= rent.start %></li>
      <li>结束时间：<%= rent.end %></li>
    </ul>
  </td>
  <td>
    <div class="span2">
      <div class="rent-rates">
        <div class="price-block">
          <span class="rmb-symbol">&#65509;</span>
          <span class="rmb"><%= rent.rate['day'] %></span>
          <span class="separator">/</span>
          <span class="unit">天</span>
        </div>
        <div class="price-block">
        </div>
        <div class="price-block">
          <span class="rmb-symbol">&#65509;</span>
          <span class="rmb"><%= rent.rate['week'] %></span>
          <span class="separator">/</span>
          <span class="unit">周</span>
        </div>
        <div class="price-block">
          <span class="rmb-symbol">&#65509;</span>
          <span class="rmb"><%= rent.rate['month'] %></span>
          <span class="separator">/</span>
          <span class="unit">月</span>
        </div>
      </div>
    </div>
  </td>
</tr>