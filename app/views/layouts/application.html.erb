<!DOCTYPE html>
<html>
<head>
  <title>Zuche</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
  <%= yield :mapapi %>
</head>
<body style="padding-top:50px">
