<!DOCTYPE html>
<html>
  <head>
    <title>车流网</title>
    <%= stylesheet_link_tag "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
    <link href="/favicon.png" rel="shortcut icon">
    <%= render 'layouts/shim' %>
    <meta charset='UTF-8'>
    <meta name="description" content="这是一个私家车出租平台">
    <meta name="keywords" content="私家车，出租，日租">
    <meta property="wb:webmaster" content="06a88c0b148dcdc2" />
  </head>
  <body id="main">

    <div class="container container-box">
      <%= render 'layouts/nav' %>
      <%= render 'shared/notice' %>
      <%= render partial: 'shared/panel_nav', locals: { active: 'nav_order'} %>
      <%= yield %>
    </div>
    <%= render 'layouts/footer' %>
    <%= yield :mapapi %>
  </body>
</html>
